select pe_fct_omschr_nl, pe_fct_omschr_fr, pe_grd_omschr_nl, pe_grd_omschr_fr from persons
where pe_fct_omschr_nl = pe_grd_omschr_nl or pe_fct_omschr_fr = pe_grd_omschr_fr
